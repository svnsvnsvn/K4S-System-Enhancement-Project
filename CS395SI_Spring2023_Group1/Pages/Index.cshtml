@page
@model IndexModel
@{
    ViewData["Title"] = "K4SVirtual Home Page";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <!--Mini Test-->
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Krumpin 4 Success </title>
    <link rel="stylesheet"  href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/CS395SI_Spring2023_Group1.styles.css" asp-append-version="true" />

</head>
<body>
  <div class="text-center">
    <h1 class="display-4" style="font-size: 40px;"> Welcome @User.Identity.Name </h1>
    <!--    
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p> 
    -->
  </div>
  <section id="picture">
      <div class="container">
            <img src="~/k4simg.jpeg" class="center">
      </div>
  </section>
  <section id="Filler">
      <div class="container">
         <div class="row">
           <h2 div="mainPageHeaders">Our Mission</h2>
            <p>
                    Krumpin 4 Success, Inc. is a youth organization dedicated to decreasing risk-taking behaviors & youth 
                    recidivism through providing creative, activities and promoting academic stability, economic sufficiency,
                    positive mental health and successful transition to productive citizenship. Krumpin 4 Success caters to 
                    youth between the ages of 13-24
            </p>
                <h2 div="mainPageHeaders">Our Vision</h2>
                <p>
                Krumpin 4 Success, Inc.’s Vision is to become the leading youth organization, which will encourage, equip,
                empower and enhance youths’ foundation for life while promoting both college and career.
             </p>
                <h2 div="mainPageHeaders">Our Core values</h2>
                <p>
                    UNITY,  TRUST,  RESPECT,  SELF-ESTEEM,  INTEGRITY, CONSISTENCY
                </p>
         </div>
      </div>
  </section>
    
  <!-- If we want to use bootstrap for whatever reason later lets keep these js folders-->
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
  </body>
</html>
